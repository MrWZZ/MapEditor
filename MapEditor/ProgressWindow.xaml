<Window x:Class="MapEditor.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapEditor"
        mc:Ignorable="d"
        Title="ProgressWindow" Height="200" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="7*"></ColumnDefinition>
            <ColumnDefinition Width="3*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ProgressBar x:Name="proProgress" Height="30"  Width="200" Minimum="0" Maximum="100"/>
        <Label Name="txtPro" Content="100/100" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="110,0,107,39"></Label>
        <Button x:Name="btnCanel" Content="取消" Grid.Column="1"  Height="30" Width="75"
                Click="btnCanel_Click"/>

    </Grid>
</Window>
